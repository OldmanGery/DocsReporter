<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="classpath:/META-INF/spring/app-context.xml"/> 
        
    <bean id="reportBuilder" class="by.creepid.docsreporter.DocReportBuilder"
          scope="singleton" lazy-init="true" depends-on="docConverters" init-method="initContext">
        <property name="templatePath">
            <value>src/test/resources/DocxProjectWithVelocity.docx</value>
        </property>
    </bean>

</beans>
